{
    "name": "commission-fees",
    "description": "Simple app to calculate commission fees",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "test": "jest --coverage --coverageDirectory=output/coverage/jest"
    },
    "jest": {
        "coverageReporters": [
            "text",
            "html"
        ],
        "roots": [
            "tests"
        ],
        "collectCoverage": true,
        "coverageDirectory": "<rootDir>/coverage/"
    },
    "dependencies": {
        "axios": "^0.20.0",
        "esm": "^3.2.25",
        "jest": "^26.4.2"
    },
    "devDependencies": {
        "eslint": "^7.9.0",
        "eslint-plugin-import": "^2.22.0"
    }
}
